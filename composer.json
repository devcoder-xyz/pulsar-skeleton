{
    "name": "devcoder-xyz/pulsar-skeleton",
    "description": "A Pulsar PHP Framework skeleton for rapid development",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Test\\App\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "F.Michel",
            "email": "dev@devcoder.xyz"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "/var/www/html/pulsar-core"
        },
        {
            "type": "path",
            "url": "/var/www/html/php-router"
        }
    ],
    "require": {
        "php": ">=7.3",
        "devcoder-xyz/pulsar-core": "dev-main",
        "devcoder.xyz/php-router": "dev-master",
        "devcoder-xyz/php-event-dispatcher": "^1.0",
        "devcoder-xyz/php-dependency-injection": "^1.0",
        "guzzlehttp/psr7": "^1.6",
        "http-interop/response-sender": "^1.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "middlewares/base-path": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    }
}
